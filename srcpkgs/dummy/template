homepage="http://www.voidlinux.org/"
shlib_provides="libgtk-4.so.1"

conflicts="gtk4 libadwaita sudo xdg-dbus-proxy xdg-utils"

do_install() {
	vmkdir usr/share/xbps.d
	for p in $conflicts; do
		echo ignorepkg=$p \
			>>${DESTDIR}/usr/share/xbps.d/99-ignore-dummy.conf
	done
}

